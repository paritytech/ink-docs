{
  "retryCount": 6,
  "fallbackRetryDelay": "30s",
  "timeout": "20s",
  "retryOn429": true,
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://ink.substrate.io/"
    }
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://crates.io"
      ],
      "headers": {
        "Accept": "text/html"
      }
    }
  ]
}
